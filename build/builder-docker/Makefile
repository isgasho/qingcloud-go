default:
	docker build -t chai2010/qingcloud-go:builder .
	@echo "ok"

pull:
	docker pull  chai2010/qingcloud-go:builder
	@echo "ok"

run:
	docker run --rm -it  chai2010/qingcloud-go:builder

clean:
	@echo "ok"
